{
  "name": "bolt-server",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "prestart": "npm run generate-api && npm run generate-gql-types && npm run knex:migrate:latest",
    "start": "gulp",
    "start:no-migration": "npm run generate-api && npm run generate-gql-types && gulp",
    "dev": "npm run start:no-migration",
    "serve": "node ./build/app.js",
    "build": "gulp build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run generate-gql-types",
    "generate-api": "node ./node_modules/@graphql-codegen/cli/dist/commonjs/cli.js --config graphql-gen.yml",
    "generate-gql-types": "node ./node_modules/graphql-schema-typescript/lib/cli.js generate-ts src/api --output src/api/graphql.types.ts --typePrefix \"\" --asyncResult true --requireResolverTypes true --noStringEnum true",
    "knex:migrate:make": "knex --knexfile src/knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "knex --knexfile src/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/knexfile.ts migrate:rollback"
  },
  "author": "Ryan Petschek, Evan Strat",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "connect-flash": "^0.1.1",
    "connect-pg-simple": "^6.0.1",
    "cookie-parser": "^1.4.4",
    "cookie-signature": "^1.1.0",
    "express": "^4.17.1",
    "express-generator": "^4.16.1",
    "express-graphql": "^0.8.0",
    "express-session": "^1.16.2",
    "fs": "0.0.1-security",
    "graphql": "^14.5.4",
    "graphql-schema-typescript": "^1.2.9",
    "graphql-server-express": "^1.4.1",
    "graphql-subscriptions": "^1.1.0",
    "gulp-copy": "^4.0.1",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^5.0.1",
    "http": "0.0.0",
    "knex": "^0.19.3",
    "marked": "^0.6.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.5.0",
    "path": "^0.12.7",
    "pg": "^7.9.0",
    "striptags": "^3.1.1",
    "subscriptions-transport-ws": "^0.9.16",
    "ts-node": "^8.3.0",
    "tslint": "^5.14.0",
    "typescript": "~3.4.1",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/typescript": "1.0.6",
    "@graphql-codegen/typescript-resolvers": "1.0.6",
    "@types/cheerio": "^0.22.11",
    "@types/compression": "0.0.36",
    "@types/connect-flash": "0.0.34",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-signature": "^1.0.2",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/express-session": "^1.15.12",
    "@types/marked": "^0.6.3",
    "@types/moment-timezone": "^0.5.12",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.0",
    "@types/passport": "^1.0.0",
    "@types/passport-oauth2": "^1.4.8",
    "@types/pg": "^7.4.14"
  }
}
